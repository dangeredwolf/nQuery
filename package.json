{
    "name": "nQuery",
    "version": "0.4.0",
    "description": "A small, highly modular alternative to jQuery",
    "scripts": {
        "babel": "babel nquery.js -o nquery.compat.js",
        "build": "rollup -c",
        "test": "npm run buildAndMinifyAll",
        "buildAndMinifyAll": "npm run buildAndMinify",
        "buildAndMinify": "npm run build && npm run minify",
        "minify": "terser out/nquery.js --comments --ecma 8 -c -m reserved=[] --output out/nquery.min.js --source-map content=nquery.js.map,url=nquery.min.js.map,root=src"
    },
    "repository": "https://github.com/dangeredwolf/nQuery",
    "author": "dangeredwolf",
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.11.6",
        "@babel/core": "^7.11.6",
        "@babel/plugin-proposal-optional-chaining": "^7.11.0",
        "@rollup/plugin-node-resolve": "^7.1.3",
        "benchmark": "^2.1.4",
        "rollup": "^2.0.0",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-terser": "^7.0.2"
    },
    "dependencies": {
        "babel-polyfill": "^6.26.0",
        "terser": "^5.3.1"
    }
}
